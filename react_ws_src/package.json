{
  "name": "XTTT-react-client",
  "description": "X Tic Tac Toe game demo front end built with React.js stack",
  "version": "0.0.1",
  "author": "Maxim Shklyar, kisla interactive, <maxim@kisla.com>",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/xims/X-TTT/react_ws_src"
  },
  "scripts": {
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:webpack_dev": "cross-env NODE_ENV=development webpack --config webpack.config.prod.js",
    "bc": "npm run build && npm run build:copyall_win",
    "bu": "npm run build && npm run build:copyupd_win",
    "bv": "npm version patch",
    "build:copyall_win": "xcopy dist\\* ..\\WS\\public /E /Y /D && xcopy static\\* ..\\WS\\public /E /Y /D",
    "build:copyupd_win": "copy dist\\bundle.js ..\\WS\\public\\ && copy dist\\style.css ..\\WS\\public\\"
  },
  "dependencies": {
    "ampersand-app": "^2.0.0",
    "get-size": "^2.0.2",
    "gsap": "^1.18.5",
    "jquery": "^3.1.0",
    "object-assign": "^4.1.0",
    "prop-types": "^15.8.1",
    "react-ga": "^2.1.1",
    "react-imageloader": "^2.1.0",
    "react-motion": "^0.4.4",
    "react-router-dom": "^4.3.1",
    "socket.io": "^1.4.8",
    "superagent": "^2.0.0",
    "validator": "^5.4.0",
    "x2js": "^2.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.9",
    "babel-core": "^6.26.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.32.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.10.1",
    "imports-loader": "^0.6.5",
    "jest": "^20.0.4",
    "jest-environment-jsdom": "^20.0.3",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-localstorage-mock": "^2.4.26",
    "node-sass": "^4.14.1",
    "proxy-middleware": "^0.15.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router": "^3.2.6",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "uglify-js": "^3.17.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-parallel-uglify-plugin": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg|png|jpg|jpeg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.xml$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
